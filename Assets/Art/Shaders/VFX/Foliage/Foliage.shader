				float3 tangentTobjectDir8 = mul( GetWorldToObjectMatrix(), float4( mul( ase_tangentToWorldFast, float3( texCoord5 ,  0.0 ) ), 0 ) ).xyz;
				float Mask23 = saturate( ( ( v.ase_normal.z + _RecolectableMask ) * _FallOffMask ) );
				float3 lerpResult27 = lerp( ( tangentTobjectDir8 * _ParametroMagicoparasolucionarlascosas2 ) , ( tangentTobjectDir8 * _ParametroMagicoparasolucionarlascosas ) , Mask23);
				
