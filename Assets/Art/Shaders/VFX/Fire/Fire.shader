				float4 ShadowCoords = float4( 0, 0, 0, 0 );

				#if defined(ASE_NEEDS_FRAG_SHADOWCOORDS)
					#if defined(REQUIRES_VERTEX_SHADOW_COORD_INTERPOLATOR)
						ShadowCoords = IN.shadowCoord;
					#elif defined(MAIN_LIGHT_CALCULATE_SHADOWS)
						ShadowCoords = TransformWorldToShadowCoord( WorldPosition );
					#endif
				#endif
				float2 texCoord23 = IN.ase_texcoord3.xy * float2( 1,1 ) + float2( 0,0 );
				float2 texCoord9 = IN.ase_texcoord3.xy * float2( 1,1 ) + float2( 0,0 );
				float temp_output_10_0 = pow( texCoord9.y , _MaskPower );
				float2 texCoord6 = IN.ase_texcoord3.xy * float2( 0.6,0.6 ) + float2( 0,0 );
				float2 panner7 = ( 1.0 * _Time.y * float2( 0,-0.59 ) + texCoord6);
				float2 panner28 = ( 1.0 * _Time.y * ( _Speed / float2( 2,2 ) ) + texCoord6);
				float2 temp_output_26_0 = ( texCoord23 + ( temp_output_10_0 * ( ( tex2D( _NoiseTexture, panner7 ).r * tex2D( _NoiseTexture, panner28 ).r ) * _NoiseIntensity ) ) );
				float2 texCoord39 = IN.ase_texcoord3.xy * float2( 1,1 ) + float2( 0,0 );
				float2 lerpResult38 = lerp( temp_output_26_0 , texCoord23 , saturate( ( ( ( 1.0 - texCoord39.y ) + _FirePos ) * _FireIntensity ) ));
				float4 temp_cast_1 = (tex2D( _MainText, lerpResult38 ).a).xxxx;
				float div33=256.0/float((int)_StepsFire);
				float4 posterize33 = ( floor( temp_cast_1 * div33 ) / div33 );
				float temp_output_34_0 = (posterize33).r;
				float4 lerpResult35 = lerp( _Color0 , _Color1 , temp_output_34_0);
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = ( lerpResult35 * IN.ase_color ).rgb;
				float Alpha = ( temp_output_34_0 * IN.ase_color.a );
				float AlphaClipThreshold = 0.5;
				float AlphaClipThresholdShadow = 0.5;

				#ifdef _ALPHATEST_ON
					clip( Alpha - AlphaClipThreshold );
				#endif

				#ifdef LOD_FADE_CROSSFADE
					LODDitheringTransition( IN.clipPos.xyz, unity_LODFade.x );
				#endif

				#ifdef ASE_FOG
					Color = MixFog( Color, IN.fogFactor );
				#endif

				return half4( Color, Alpha );
			}

			ENDHLSL
